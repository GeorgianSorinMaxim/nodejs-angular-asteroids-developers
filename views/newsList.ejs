<!doctype html>
<html ng-app="myApp">
<head>
    <title>NEWS List</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>

    <!-- okeyNav -->
    <link rel="stylesheet" href="./css/okayNav.min.css" media="screen">
    <script src="./js/jquery.okayNav.js"></script>

    <link rel="stylesheet" href="../../css/style.css">

    <script>
    var myApp = angular.module("myApp", []);

    myApp.controller("newsController", function($scope, $http) {

        $scope.newsList = <%- JSON.stringify(newsList) %>;
        console.log($scope.newsList);
    });
    </script>
</head>
<body ng-controller="newsController">
    <header id="header">
        <nav role="navigation" id="nav-main" class="okayNav">
            <ul>
                <li><a href="/news">NEWS</a></li>
                <li><a href="/newsList">NEWS List</a></li>
                <li><a href="/patients">Patients List</a></li>
                <li><a href="/send">Send Message</a></li>
            </ul>
        </nav>
    </header>

    </br>

    <main>
      <h1>Self-measured NEWS List</h1>
      <div ng-repeat="news in newsList"/>
          <form class="patient" action="">
              Patient: <span class="spanData">{{news.cpr}}</span> - <span class="spanData">{{news.firstname}} {{news.lastname}}</span>, Respiration Rate: <span class="spanData">{{news.respiration}}</span>, Oxygen Saturations: <span class="spanData">{{news.oxygenSat}}</span>, Any Supplement Oxygen: <span class="spanData">{{news.oxygen}}</span>, Temperature: <span class="spanData">{{news.temperature}}</span>, Systolic BP: <span class="spanData">{{news.systolic}}</span>, Heart Rate: <span class="spanData">{{news.heartRate}}</span>, Level of Consciousness: <span class="spanData">{{news.consciousness}}</span>.
          </form>
      </div>
  </main>
<script>
  var navigation = $('#nav-main').okayNav();
</script>
</body>
</html>
